<resources>

    <!-- Base application theme. -->
    <style courseName="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item courseName="colorPrimary">@color/colorPrimary</item>
        <item courseName="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item courseName="colorAccent">@color/colorAccent</item>


        <item courseName="android:windowNoTitle">true</item>
        <item courseName="android:windowFullscreen">true</item>

    </style>


    <style courseName="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.ActionBar" />

    <style courseName="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />


    <style courseName="MaterialSwitch">
        <item courseName="sw_trackSize">24dp</item>
        <item courseName="sw_trackCap">round</item>
        <item courseName="sw_thumbRadius">16dp</item>
        <item courseName="sw_thumbElevation">2dp</item>
        <item courseName="sw_animDuration">@android:integer/config_shortAnimTime</item>
        <item courseName="sw_interpolator">@android:anim/decelerate_interpolator</item>
    </style>


</resources>
