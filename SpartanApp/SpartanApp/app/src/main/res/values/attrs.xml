<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable courseName="StickyScrollView">
        <attr courseName="stuckShadowHeight" format="dimension" />
        <attr courseName="stuckShadowDrawable" format="reference" />
    </declare-styleable>


    <declare-styleable courseName="ChartAttrs">

        <attr courseName="chart_axisThickness" format="dimension"/>
        <attr courseName="chart_axisColor" format="reference|color"/>
        <attr courseName="chart_axisBorderSpacing" format="dimension"/>
        <attr courseName="chart_axisTopSpacing" format="dimension"/>

        <attr courseName="chart_labels" format="boolean"/>
        <attr courseName="chart_labelColor" format = "reference|color"/>
        <attr courseName="chart_fontSize" format = "dimension"/>
        <attr courseName="chart_typeface" format="string"/>

        <attr courseName="chart_shadowColor" format="reference|color"/>
        <attr courseName="chart_shadowDx" format = "dimension"/>
        <attr courseName="chart_shadowDy" format = "dimension"/>
        <attr courseName="chart_shadowRadius" format = "dimension"/>

    </declare-styleable>

    <declare-styleable courseName="BarChartAttrs">
        <attr courseName="chart_barSpacing" format="dimension"/>
        <attr courseName="chart_setSpacing" format="dimension"/>
    </declare-styleable>
</resources>

